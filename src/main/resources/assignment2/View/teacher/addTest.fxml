<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="CENTER" prefHeight="525.0" prefWidth="645.0" spacing="20.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="assignment2.View.addTestController">

   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
   <children>
      <Pane prefHeight="589.0" prefWidth="603.0">
         <children>
            <Label layoutX="250.0" layoutY="-18.0" text="New Test">
               <font>
                  <Font size="25.0" />
               </font>
            </Label>
            <Pane layoutX="-2.0" layoutY="28.0" prefHeight="435.0" prefWidth="615.0" style="-fx-border-style: solid;">
               <children>
                  <Label fx:id="lblOp411" layoutX="121.0" layoutY="18.0" prefHeight="17.0" prefWidth="63.0" text="Test Name" />
                  <TextField fx:id="txtTestName" layoutX="189.0" layoutY="14.0" prefHeight="25.0" prefWidth="356.0" />
                  <ListView fx:id="lvQuestionBank" layoutX="6.0" layoutY="79.0" prefHeight="344.0" prefWidth="284.0" />
                  <ListView fx:id="lvQuestionsInTest" layoutX="325.0" layoutY="79.0" prefHeight="200.0" prefWidth="284.0" />
                  <Label fx:id="lblOp4111" layoutX="92.0" layoutY="62.0" prefHeight="17.0" prefWidth="92.0" text="Question Bank" />
                  <Label fx:id="lblOp41111" layoutX="424.0" layoutY="62.0" prefHeight="17.0" prefWidth="92.0" text="Questions in Test" />
                  <Button fx:id="btnAdd" layoutX="292.0" layoutY="114.0" mnemonicParsing="false" onAction="#addQuestions" prefHeight="25.0" prefWidth="31.0" text="&gt;" />
                  <Button fx:id="btnRemove" layoutX="292.0" layoutY="167.0" mnemonicParsing="false" onAction="#removeQuestions" prefHeight="25.0" prefWidth="31.0" text="&lt;" />
               </children>
            </Pane>
            <Button fx:id="btnSave" layoutX="212.0" layoutY="468.0" mnemonicParsing="false" onAction="#saveTest" prefHeight="25.0" prefWidth="174.0" text="Add Test" />
            <Button fx:id="btnBack" layoutX="512.0" layoutY="-7.0" mnemonicParsing="false" onAction="#back" prefHeight="25.0" prefWidth="86.0" text="Back" />
         </children>
      </Pane>
   </children>
</VBox>
